{
  "name": "pm2-discord",
  "version": "1.0.0",
  "description": "A PM2 module to emit events and logs to discord",
  "main": "dist/index.mjs",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "npm run build && NODE_ENV=test node --test tests/unit",
    "test:integration": "npm run build && NODE_ENV=test node --test tests/integration",
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FranciscoG/pm2-discord"
  },
  "keywords": [
    "pm2",
    "discord"
  ],
  "author": "Francisco (https://github.com/FranciscoG)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/FranciscoG/pm2-discord/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/FranciscoG/pm2-discord/issues"
  },
  "homepage": "https://github.com/FranciscoG/pm2-discord",
  "dependencies": {
    "node-fetch": "3.3.2",
    "pm2": "6.0.14",
    "pmx": "1.6.8",
    "strip-ansi": "7.1.2"
  },
  "devDependencies": {
    "@types/axon": "^2.0.5",
    "@types/node": "^16.0.0",
    "typescript": "5.9.3"
  }
}